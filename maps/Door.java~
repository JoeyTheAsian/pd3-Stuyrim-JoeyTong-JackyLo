public class Door extends MapObject{

    public Door(ImageIcon image){
	canBreak = false;
	canWalkOn = false;
	canUse = true;
	texture = image;
    }
    public setWalkTrue(){
	canWalkOn = true;
    }
    public setWalkFalse(){
	canWalkOn = false;
    }
    public setWalk(boolean b){
	canWalkOn = b;
    }
}
